{
  "name": "@st1ggy/linter-config",
  "version": "2.42.0",
  "description": "ESLint, Stylelint and Prettier configs for React, Vue [WIP] and Svelte [WIP]",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/st1ggy/linter-config.git"
  },
  "keywords": [
    "eslint",
    "prettier",
    "stylelint"
  ],
  "author": "Dmitrii Bragin (st1ggy)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/St1ggy/linter-config/issues"
  },
  "homepage": "https://github.com/St1ggy/linter-config#readme",
  "dependencies": {
    "@next/eslint-plugin-next": "^16.1.6",
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@stylistic/eslint-plugin-migrate": "^4.4.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-sonarjs": "^3.0.6",
    "eslint-plugin-svelte": "^3.14.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "find-up-simple": "^1.0.1",
    "patch-package": "^8.0.1",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "stylelint": "^17.1.1",
    "stylelint-config-clean-order": "^8.0.0",
    "stylelint-config-recommended": "^18.0.0",
    "stylelint-config-recommended-scss": "^17.0.0",
    "stylelint-config-sass-guidelines": "^13.0.0",
    "stylelint-no-unsupported-browser-features": "^8.1.1",
    "stylelint-order": "^7.0.1",
    "stylelint-prettier": "^5.0.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "types": "./index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    },
    "./eslint-next": {
      "types": "./index.d.ts",
      "import": "./src/eslint/eslint.config.next.js",
      "default": "./src/eslint/eslint.config.next.js"
    },
    "./eslint-react": {
      "types": "./index.d.ts",
      "import": "./src/eslint/eslint.config.react.js",
      "default": "./src/eslint/eslint.config.react.js"
    },
    "./eslint-svelte": {
      "types": "./index.d.ts",
      "import": "./src/eslint/eslint.config.svelte.js",
      "default": "./src/eslint/eslint.config.svelte.js"
    },
    "./eslint-common": {
      "types": "./index.d.ts",
      "import": "./src/eslint/eslint.config.common.js",
      "default": "./src/eslint/eslint.config.common.js"
    },
    "./prettier-common": {
      "types": "./index.d.ts",
      "import": "./src/prettier/prettier.config.common.js",
      "default": "./src/prettier/prettier.config.common.js"
    },
    "./prettier-svelte": {
      "types": "./index.d.ts",
      "import": "./src/prettier/prettier.config.svelte.js",
      "default": "./src/prettier/prettier.config.svelte.js"
    },
    "./stylelint-scss": {
      "types": "./index.d.ts",
      "import": "./src/stylelint/stylelint.config.scss.js",
      "default": "./src/stylelint/stylelint.config.scss.js"
    }
  },
  "files": [
    "src",
    "scripts",
    "index.d.ts"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "next": "^16.0.0",
    "prettier": "^3.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "stylelint": "^16.0.0",
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    },
    "next": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "stylelint": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  }
}
